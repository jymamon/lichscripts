Creatures.init_db

creatures_to_add_or_update.each{|critter|
    Creatures.insert(
        Creature.new(
            critter[:id],
            critter[:level],
            critter[:name],
            critter[:skin],
            critter[:undead],
            critter[:noncorp],
            critter[:hated]
        )
    );
}

Areas.init_db

i = 0

areas_to_add_or_update.each{|area|
    Areas.delete(area[:id])

    Areas.insert(
        Area.new(
            area[:id],
            area[:start],
            area[:boundaries],
            area[:creatures]
        )
    );
    i += 1
}

echo "Added #{i} areas"

AtlasDatabase.close_db()
